{
  "name": "indra_reto",
  "version": "1.0.0",
  "description": "Reto de Indra",
  "scripts": {
    "start": "npx sls offline -s dev",
    "build": "npx sls package",
    "lint": "eslint ./src/.",
    "prepush": "npm test",
    "pretest": "npm run lint",
    "test": "NODE_ENV=testing jest ./src/. --coverage",
    "docs": "npx sls openapi generate",
    "prepare": "husky install"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ]
  },
  "keywords": [
    "lambda",
    "indra",
    "aws"
  ],
  "author": "Javier Galarza<jegj57@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "ajv": "8.11.0",
    "axios": "0.27.2",
    "mysql": "2.18.1"
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-extended": "^3.0.2",
    "serverless-dotenv-plugin": "^4.0.1",
    "serverless-esbuild": "^1.31.0",
    "serverless-offline": "^8.8.0",
    "serverless-openapi-documentation": "^0.4.0"
  }
}